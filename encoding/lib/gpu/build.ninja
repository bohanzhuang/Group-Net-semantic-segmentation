cuda_flags = -DTORCH_EXTENSION_NAME=enclib_gpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /fast/users/a1675776/anaconda3/envs/binary_segmentation/lib/python3.6/site-packages/torch/lib/include -isystem /fast/users/a1675776/anaconda3/envs/binary_segmentation/lib/python3.6/site-packages/torch/lib/include/torch/csrc/api/include -isystem /fast/users/a1675776/anaconda3/envs/binary_segmentation/lib/python3.6/site-packages/torch/lib/include/TH -isystem /fast/users/a1675776/anaconda3/envs/binary_segmentation/lib/python3.6/site-packages/torch/lib/include/THC -isystem /apps/software/CUDA/9.0.176/include -isystem /fast/users/a1675776/anaconda3/envs/binary_segmentation/include/python3.6m -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --compiler-options '-fPIC' --expt-extended-lambda -std=c++11
ldflags = -shared -L/apps/software/CUDA/9.0.176/lib64 -lcudart

rule compile
